<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@+id/navigation_login">

    <fragment
        android:id="@+id/navigation_login"
        android:name="com.example.sipaddy.presentation.auth.login.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_navigation_login_to_navigation_register"
            app:destination="@id/navigation_register"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_home"
            app:destination="@id/navigation_home"
            app:popUpTo="@id/main_navigation"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.sipaddy.presentation.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_navigation_home_to_pengaduanTanamanFragment"
            app:destination="@id/navigation_pengaduan_tanaman" />

        <action
            android:id="@+id/action_navigation_home_to_historyPengaduanTanamanFragment"
            app:destination="@id/historyPengaduanTanamanFragment" />
        <action
            android:id="@+id/action_navigation_home_to_poptPengaduanTanamanFragment"
            app:destination="@id/poptPengaduanTanamanFragment" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_login"
            app:destination="@id/navigation_login"
            app:popUpTo="@id/main_navigation"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_navigation_home_to_navigation_predict_disease"
            app:destination="@id/navigation_predict_disease" />

    </fragment>


    <fragment
        android:id="@+id/navigation_register"
        android:name="com.example.sipaddy.presentation.auth.register.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_navigation_register_to_navigation_login"
            app:destination="@id/navigation_login"
            app:popUpTo="@id/navigation_login"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/navigation_history"
        android:name="com.example.sipaddy.presentation.history.HistoryFragment"
        android:label="fragment_history"
        tools:layout="@layout/fragment_history">
        <action
            android:id="@+id/action_navigation_history_to_navigation_result"
            app:destination="@id/navigation_result"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.sipaddy.presentation.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_navigation_profile_to_settingAkunFragment"
            app:destination="@id/settingAkunFragment" />
    </fragment>
    <fragment
        android:id="@+id/navigation_diagnose"
        android:name="com.example.sipaddy.presentation.home.diagnose.DiagnoseFragment"
        android:label="fragment_diagnose"
        tools:layout="@layout/fragment_diagnose">
        <action
            android:id="@+id/action_navigation_diagnose_to_resultFragment"
            app:destination="@id/navigation_result"
            app:enterAnim="@anim/from_right"
            app:exitAnim="@anim/to_left"
            app:popEnterAnim="@anim/from_left"
            app:popExitAnim="@anim/to_right" />
    </fragment>

    <fragment
        android:id="@+id/navigation_result"
        android:name="com.example.sipaddy.presentation.home.diagnose.result.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result">

        <argument
            android:name="predictionId"
            app:argType="string" />
        <action
            android:id="@+id/action_navigation_result_to_navigation_predict_disease"
            app:destination="@id/navigation_predict_disease"
            app:popUpTo="@id/navigation_home"

            />
    </fragment>

    <fragment
        android:id="@+id/navigation_pengaduan_tanaman"
        android:name="com.example.sipaddy.presentation.pengaduantanaman.PengaduanTanamanFragment"
        android:label="fragment_pengaduan_tanaman"
        tools:layout="@layout/fragment_pengaduan_tanaman" />
    <fragment
        android:id="@+id/historyPengaduanTanamanFragment"
        android:name="com.example.sipaddy.presentation.pengaduantanaman.history.HistoryPengaduanTanamanFragment"
        android:label="fragment_history_pengaduan_tanaman"
        tools:layout="@layout/fragment_history_pengaduan_tanaman">
        <action
            android:id="@+id/action_historyPengaduanTanamanFragment_to_detailPengaduanTanamanFragment"
            app:destination="@id/detailPengaduanTanamanFragment" />

    </fragment>

    <fragment
        android:id="@+id/detailPengaduanTanamanFragment"
        android:name="com.example.sipaddy.presentation.pengaduantanaman.detail.DetailPengaduanTanamanFragment"
        android:label="fragment_detail_pengaduan_tanaman"
        tools:layout="@layout/fragment_detail_pengaduan_tanaman">

        <argument
            android:name="id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/poptDetailPengaduanTanamanFragment"
        android:name="com.example.sipaddy.presentation.popt.detail.PoptDetailPengaduanTanamanFragment"
        android:label="fragment_popt_detail_pengaduan_tanaman"
        tools:layout="@layout/fragment_popt_detail_pengaduan_tanaman">
        <argument
            android:name="id"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/poptPengaduanTanamanFragment"
        android:name="com.example.sipaddy.presentation.popt.PoptPengaduanTanamanFragment"
        android:label="fragment_popt_pengaduan_tanaman"
        tools:layout="@layout/fragment_popt_pengaduan_tanaman">
        <action
            android:id="@+id/action_poptPengaduanTanamanFragment_to_poptDetailPengaduanTanamanFragment"
            app:destination="@id/poptDetailPengaduanTanamanFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingAkunFragment"
        android:name="com.example.sipaddy.presentation.profile.setting.SettingAkunFragment"
        android:label="fragment_setting_akun"
        tools:layout="@layout/fragment_setting_akun" />
    <fragment
        android:id="@+id/navigation_camerax"
        android:name="com.example.sipaddy.presentation.home.camera.CameraXFragment"
        android:label="fragment_camera_x"
        tools:layout="@layout/fragment_camera_x" />
    <fragment
        android:id="@+id/navigation_predict_disease"
        android:name="com.example.sipaddy.presentation.home.predict.PredictDiseaseFragment"
        android:label="fragment_predict_disease"
        tools:layout="@layout/fragment_predict_disease">
        <action
            android:id="@+id/action_predictDiseaseFragment_to_navigation_camerax"
            app:destination="@id/navigation_camerax" />
        <action
            android:id="@+id/action_navigation_predict_disease_to_navigation_result"
            app:destination="@id/navigation_result"
            app:popUpTo="@id/navigation_predict_disease"
            app:popUpToInclusive="true"
            />
    </fragment>


</navigation>